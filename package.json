{
  "name": "@buff-beacon-project/curby-client",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "version": "0.0.12",
  "type": "module",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "browser": "./dist/index.js",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "node": "./dist/index.js",
      "bun": "./src/index.ts",
      "types": "./src/index.d.ts"
    }
  },
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "author": "Jasper Palfree <jasper.palfree@colorado.edu>",
  "license": "MIT",
  "keywords": [
    "curby",
    "twine",
    "beacon",
    "randomness"
  ],
  "homepage": "https://random.colorado.edu",
  "bugs": {
    "url": "https://github.com/buff-beacon-project/curby-js-client/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/buff-beacon-project/curby-js-client.git"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "docs": "typedoc --out docs --exclude *.test.ts --plugin typedoc-plugin-markdown src",
    "prepublishOnly": "bun run build && bun run docs",
    "release": "bunx bumpp --no-push"
  },
  "devDependencies": {
    "@aryzing/bun-mock-fetch": "^0.2.2",
    "bun-types": "latest",
    "tsup": "^8.3.0",
    "typedoc": "^0.26.10",
    "typedoc-plugin-markdown": "^4.2.9",
    "typescript": "^5.4.2"
  },
  "peerDependencies": {},
  "dependencies": {
    "@buff-beacon-project/rand-utils": "0.1.1",
    "@twine-protocol/twine-core": "^0.0.1-alpha.1",
    "@twine-protocol/twine-http-store": "^0.0.1-alpha.1",
    "itty-fetcher": "^0.9.4",
    "jssha": "^3.3.1",
    "streaming-iterables": "^8.0.1"
  }
